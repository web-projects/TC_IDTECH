<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

    <appSettings>
        <!-- APPLICATION OPTIONS -->
        <add key="tc_show_settings_tab" value="true" />
        <add key="tc_show_configuration_tab" value="true" />
        <add key="tc_show_raw_mode_tab" value="true" />
        <add key="tc_show_json_tab" value="true" />
        <!-- DEVICE CONFIGURATION -->
        <add key="tc_read_terminal_info" value="true" />
        <add key="tc_read_terminal_data" value="true" />
        <add key="tc_read_encryption" value="true" />
        <add key="tc_read_capk_settings" value="false" />
        <add key="tc_read_aid_settings" value="false" />
        <add key="tc_read_msr_settings" value="false" />
        <!-- MSR READ -->
        <add key="tc_read_msr_timeout" value="30000" />
        <!-- TRANSACTION TIMER -->
        <add key="tc_transaction_timeout" value="1000" />
        <add key="tc_transaction_collection_timeout" value="3000" />
        <add key="tc_minimum_transaction_length" value="950" />
        <!-- DEVICE CONFIGURATION -->
        <add key="IPA.DAL.Device.AcceptedComPorts" value="COM109,COM110,COM111,COM112,COM113" />
        <add key="IPA.DAL.Device.COMBaudRate" value="115200" />
        <add key="IPA.DAL.Device.COMDataBits" value="8" />
        <add key="IPA.DAL.Application.Client.Logging" value="true" />
    </appSettings>

  <runtime>

    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">

      <dependentAssembly>

        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />

      </dependentAssembly>

    </assemblyBinding>

  </runtime>
</configuration>